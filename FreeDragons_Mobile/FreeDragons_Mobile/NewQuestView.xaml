<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FreeDragons_Mobile.NewQuestView">
  <ContentView.Content>
      <StackLayout>
            <Label Text="Create new Quest" FontSize="Large"/>
            <Label  Text="Title"></Label>
            <Entry x:Name="Title" Text=""></Entry>
            <Editor x:Name="Description"  Text=""></Editor>
            <StackLayout Orientation="Horizontal">
                <Button x:Name="OK" Text="OK"></Button>
                <Button x:Name="Cancel" Text="Cancel"></Button>
            </StackLayout>
        </StackLayout>
  </ContentView.Content>
</ContentView>